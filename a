[1m+++ b/javascript/Validator.js[m
[36m@@ -0,0 +1,107 @@[m
[32m+[m[32mclass Validator {[m
[32m+[m[32m    constructor(config, formID) {[m
[32m+[m[32m        this.elementsConfig = config;[m
[32m+[m[32m        this.formID = formID;[m
[32m+[m[32m        this.errors = {};[m
[32m+[m
[32m+[m[32m        this.generateErrorsObject();[m
[32m+[m[32m        this.inputListener();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    generateErrorsObject() {[m
[32m+[m[32m        for (let field in this.elementsConfig) {[m
[32m+[m[32m            this.errors[field] = [];[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    inputListener() {[m
[32m+[m[32m        let inputSelector = this.elementsConfig;[m
[32m+[m
[32m+[m[32m        for (let field in inputSelector) {[m
[32m+[m[32m            let el = document.querySelector(`${this.formID} input[name="${field}"]`);[m
[32m+[m
[32m+[m[32m            el.addEventListener('input', this.validate.bind(this));[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    validate(e) {[m
[32m+[m[32m        let elFields = this.elementsConfig;[m
[32m+[m
[32m+[m[32m        let field = e.target;[m
[32m+[m[32m        let fieldName = field.getAttribute('name');[m
[32m+[m[32m        let fieldValue = field.value;[m
[32m+[m
[32m+[m[32m        this.errors[fieldName] = [];[m
[32m+[m
[32m+[m[32m        if (elFields[fieldName].required) {[m
[32m+[m[32m            if (fieldValue === '') {[m
[32m+[m[32m                this.errors[fieldName].push('Polje je prazno');[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (elFields[fieldName].email) {[m
[32m+[m[32m            if (!this.validateEmail(fieldValue)) {[m
[32m+[m[32m                this.errors[fieldName].push('Neispravna email adresa');[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (fieldValue.length < elFields[fieldName].minLength || fieldValue.length > elFields[fieldName].maxLength) {[m
[32m+[m[32m            this.errors[fieldName].push(`Polje mora imati minimalno ${elFields[fieldName].minLength} i maksimalno ${elFields[fieldName].maxLength} karaktera`);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (elFields[fieldName].matching) {[m
[32m+[m[32m            let matchingEl = document.querySelector(`${this.formID} input[name="${elFields[fieldName].matching}"]`);[m
[32m+[m
[32m+[m[32m            if (fieldValue !== matchingEl.value) {[m
[32m+[m[32m                this.errors[fieldName].push('Lozinke se ne poklapaju');[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (this.errors[fieldName].length === 0) {[m
[32m+[m[32m                this.errors[fieldName] = [];[m
[32m+[m[32m                this.errors[elFields[fieldName].matching] = [];[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this.populateErrors(this.errors);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    validationPassed() {[m
[32m+[m[32m        for (let key of Object.keys(this.errors)) {[m
[32m+[m[32m            if (this.errors[key].length > 0) {[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return true;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    populateErrors(errors) {[m
[32m+[m[32m        for (const elem of document.querySelectorAll('ul')) {[m
[32m+[m[32m            if (elem.id !== 'linkovi') {[m
[32m+[m[32m                elem.remove();[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        for (let key of Object.keys(errors)) {[m
[32m+[m[32m            let parentElement = document.querySelector(`${this.formID} input[name="${key}"]`).parentElement;[m
[32m+[m[32m            let errorsElement = document.createElement('ul');[m
[32m+[m[32m            parentElement.appendChild(errorsElement);[m
[32m+[m
[32m+[m[32m            errors[key].forEach(error => {[m
[32m+[m[32m                let li = document.createElement('li');[m
[32m+[m[32m                li.innerText = error;[m
[32m+[m
[32m+[m[32m                errorsElement.appendChild(li);[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    validateEmail(email) {[m
[32m+[m[32m        if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(email)) {[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
